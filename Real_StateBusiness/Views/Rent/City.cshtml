@model Real_StateBusiness.Models.Rent

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>City</title>
</head>
<body>
    <h1>City Names</h1>
    @{ 
        foreach(string i in ViewBag.City)
        {
            <h3>@Html.ActionLink(i,"city1",new { cy=i})</h3>
        }
    }
</body>
</html>
